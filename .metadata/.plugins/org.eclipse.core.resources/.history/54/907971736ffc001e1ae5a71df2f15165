package product;

public class ProductVO {//자바빈(1.필드 2.메소드)
	
	private  String code;
	private String name;
	private int price;
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPricr() {
		return price;
	}
	public void setPricr(int pricr) {
		this.price = price;
	}
	
	@Override
	public String toString() {
		return "ProductVO [code=" + code + ", name=" + name + ", pricre=" + price + "]";
	}
	
	
}
