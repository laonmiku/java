package am;

public class Sub01 {
	public static void run() {
		Car car1=new Car();
		car1.company="현대";
		car1.model="그랜저";
		car1.color="검정";
		car1.distance=60000;
		car1.date="2021-06";
		car1.print();
		
		car1=new Car();
		car1.company="쌍용";
		car1.model="티볼리";
		car1.color="파랑";
		car1.distance=80000;
		car1.date="2020-07";
		car1.print();
	
	}
}
