package am;

import java.util.*;

public class Car {
	//필드
	String company;
	String model;
	String color;
	int distance;
	String date;
	
	
	//생성자
	public Car(){
		
	}
	
	//메소드
	public void print() {
		System.out.println("회사 : "+company);
		System.out.println("모델 : "+model);
		System.out.println("색상 : "+color);
		System.out.println("주행거리 : "+distance);
		System.out.println("연식 : "+date);
		
	}
	
}
