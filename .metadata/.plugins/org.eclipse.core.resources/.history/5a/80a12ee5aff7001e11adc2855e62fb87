package am;

public class Sub02 {
	public static void run() {
		System.out.println("클래스란?");

		Student s1= new Student();//생성자1
		s1.no="100";
		s1.name="홍길동";
		s1.address="인천";
		s1.phone="010-0000-1111";
		s1.print();
		
		System.out.println("-----------------------------------------");
		
		Student s2=new Student("101","이순신");//생성자2
		s2.print();
	}
}
