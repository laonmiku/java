package product;

public class ProductVO {//자바빈(1.필드 2.메소드)
	
	private  String code;
	private String name;
	private int pricre;
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPricr() {
		return pricre;
	}
	public void setPricr(int pricr) {
		this.pricre = pricr;
	}
	
	
}
